[submodule "lib/glfw"]
	path = lib/glfw
	url = https://github.com/glfw/glfw.git
[submodule "lib/glfw-wrapper"]
	path = lib/glfw-wrapper
	url = https://github.com/cpp-wrappers/glfw-wrapper
[submodule "lib/opengl-wrapper"]
	path = lib/opengl-wrapper
	url = https://github.com/cpp-wrappers/opengl-wrapper.git
[submodule "lib/openal-wrapper"]
	path = lib/openal-wrapper
	url = https://github.com/cpp-wrappers/openal-wrapper.git
[submodule "lib/openal-soft"]
